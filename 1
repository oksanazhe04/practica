def binary_search(sorted_list, target):
    low = 0
    high = len(sorted_list) - 1

    while low <= high:
        mid = (low + high) // 2
        number = sorted_list[mid]
        
        if number == target:
            return mid
        elif number < target:
            low = mid + 1
        else:
            high = mid - 1
    
    return -1 

sorted_list = list(range(1, 100))
target = 68

index = binary_search(sorted_list, target)

if index != -1:
    print(f"Елемент {target} знайдено в позиції {index}.")
else:
    print(f"Елемент {target} не знайдено в списку.")
