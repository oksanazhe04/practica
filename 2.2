def insertion_sort(arr):
    for i in range(1, len(arr)):
        current_value = arr[i]
        j = i - 1
        while j >= 0 and arr[j] > current_value:
            arr[j + 1] = arr[j]
            j -= 1
        arr[j + 1] = current_value
    return arr

numbers = [12, 4, 6, 3, 10, 1, 8, 20, 15, 17, 9, 5, 2, 7, 14, 11, 18, 13, 19, 16]
sorted_numbers = insertion_sort(numbers.copy())
print(f'Відсортований список: {sorted_numbers}')
