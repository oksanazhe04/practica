def selection_sort(data):
    n = len(data)
    
    for i in range(n):
        min_index = i
        
        for j in range(i + 1, n):
            if data[j][1] < data[min_index][1]:
                min_index = j
        
        data[min_index], data[i] = data[i], data[min_index]
    
    return data

data = [
    ("ООП", 243),
    ("Технології в бізнесі", 200),
    ("Економетрика", 391),
    ("Менеджмент", 147),
    ("Маркетинг", 94),
    ("Міжнародна економіка", 361),
    ("ООМ", 75),
    ("Англійська мова", 237),
    ("Гроші та кредит", 450)
]

sorted_data = selection_sort(data)

for item in sorted_data:
    print(f"{item[0]} - {item[1]}")
